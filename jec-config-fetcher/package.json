{
	"name": "jec-config-fetcher",
	"version": "0.0.1",
	"description": "For reading the .jecrc.js config file that is used to configure all command line JEC tools",
	"main": "index.js",
	"license": "MIT",
	"files": [
		"lib"
	],
	"scripts": {
		"build": "NODE_ENV=production babel src --out-dir lib",
		"format": "react-scripts format",
		"precommit": "yarn format && git add -u && yarn build",
		"watch": "NODE_ENV=production babel --source-maps inline --watch src --out-dir lib",
		"watch-parcel": "parcel index.src.js"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-node-source-map-support": "^0.0.1",
		"babel-plugin-preval": "^1.6.3",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-codogo": "^2.0.1",
		"babel-preset-env": "^1.6.1",
		"babel-preset-flow": "^6.23.0",
		"codogo-react-scripts": "^1.0.14-delta.84",
		"eslint": "^4.15.0",
		"eslint-config-codogo": "^2.1.1",
		"flow-bin": "^0.63.1",
		"parcel-bundler": "^1.4.1",
		"prettier": "^1.10.2"
	},
	"dependencies": {
		"ramda": "^0.25.0"
	}
}
